<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="600.0" style="-fx-background-color: lightgrey;" stylesheets="@Style.css"
            xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="View.MyHomePageView">
   <AnchorPane prefHeight="606.0" prefWidth="292.0" style="-fx-background-color: lightgrey;">
      <ImageView fx:id="brandingImageView" fitHeight="256.0" fitWidth="261.0" layoutX="18.0" layoutY="61.0"
                 pickOnBounds="true" preserveRatio="true">
         <Image url="@../../images/logo.png"/>
      </ImageView>
   </AnchorPane>
   <AnchorPane layoutX="292.0" prefHeight="606.0" prefWidth="309.0"
               style="-fx-background-color: #a9a9a9 #a9a9a9 #a9a9a9 #a9a9a9;">
      <Button fx:id="yourCoursesButton" layoutX="46.0" layoutY="157.0" mnemonicParsing="false"
              onAction="#loadMyCoursesView" prefHeight="41.0" prefWidth="219.0"
              style="-fx-background-color: #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7;"
              text="Your Courses" textFill="#0d0c0c">
         <font>
            <Font size="21.0"/>
         </font>
      </Button>
      <Button fx:id="createMeetingButton" layoutX="46.0" layoutY="344.0" mnemonicParsing="false"
              onAction="#loadNewMeetUpView" prefHeight="41.0" prefWidth="219.0"
              style="-fx-background-color: #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7;"
              text="Create Meeting" textFill="#0d0c0c">
         <font>
            <Font size="21.0"/>
         </font>
      </Button>
      <Button fx:id="joinMeetingButton" layoutX="46.0" layoutY="397.0" mnemonicParsing="false"
              onAction="#loadMeetUpCalendarView" prefHeight="41.0" prefWidth="219.0"
              style="-fx-background-color: #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7;"
              text="Join Meeting" textFill="#0d0c0c">
         <font>
            <Font size="21.0"/>
         </font>
      </Button>
      <Button fx:id="cancelButton" layoutX="55.0" layoutY="526.0" mnemonicParsing="false" onAction="#cancelButton"
              prefHeight="13.0" prefWidth="176.0"
              style="-fx-background-color: #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7;"
              text="Log out" textFill="#0d0c0c">
         <font>
            <Font size="18.0"/>
         </font>
      </Button>
      <Button fx:id="yourMeetingsButton" layoutX="46.0" layoutY="207.0" mnemonicParsing="false"
              onAction="#loadMyMeetUpsView" prefHeight="41.0" prefWidth="219.0"
              style="-fx-background-color: #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7 #3f8de7#3f8de7;"
              text="Your Meetings" textFill="#0d0c0c">
         <font>
            <Font size="20.0"/>
         </font>
      </Button>
      <Label layoutX="46.0" layoutY="316.0" text="Create or join a meeting!">
         <font>
            <Font size="15.0"/>
         </font>
      </Label>
      <Label fx:id="courseMeetingLabel" layoutX="46.0" layoutY="107.0" prefHeight="41.0" prefWidth="250.0"
             text="See and edit your courses and meetings">
         <font>
            <Font size="15.0"/>
         </font>
      </Label>
      <Label fx:id="userLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="30.0" layoutY="38.0"
             prefHeight="35.0" prefWidth="250.0" text="Welcome">
         <font>
            <Font size="19.0"/>
         </font>
      </Label>
   </AnchorPane>
</AnchorPane>
